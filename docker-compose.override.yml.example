version: "3"

services:
  cap:
    environment:
      - CAP_Model::Access::Derivative_key=<insert key>
      - 'CAP_Model::Access::Derivative_password=<insert password>'
      - CAP_Model::Access::Download_container=<swift container url>
      - CAP_Model::Access::Download_tempURLKey=<swift tempurl key>
      # You can also edit defaults, e.g.
      - CAP_Model::CouchCache_interval=3
      - CAP_cookie_domain=mamirolle.canadiana.ca
      - LOGLEVELSCREEN=DEBUG
